{
  "version": 2,
  "functions": {
    "api/*.py": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/update_dreps",
      "schedule": "0 0 * * *" 
    },
    {
      "path": "/api/update_spos_and_supply",
      "schedule": "0 0 * * *"  
    }
  ]
}